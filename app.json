{
  "name": "Duckbot",
  "description": "Powerfull telegram bot",
  "keywords": [
    "bot",
    "telegram bot",
    "typescript telegram bot"
  ],
  "website": "https://butthx.vercel.app/",
  "repository": "https://github.com/butthx/duckbot",
  "env": {
    "BOT_TOKEN": {
      "description": "TOKEN BOT FROM BOT FATHER",
      "required": true
    },
    "MONGGODB": {
      "description": "MONGGO DB URI FOR DATABASE",
      "required":true
    },
    "SPAMWATCH_TOKEN":{
      "description":"API KEY FROM SPAMWATCH",
      "required":true
    },
    "ERROR_LOG" : {
      "description":"CHAT_ID TO REPORT ERROR",
      "required":true
    },
    "OCR_API" :{
      "description":"API KEY FROM OCR SPACE",
      "required":true
    },
    "WEBHOOK_URL":{
      "description":"URL FROM YOUR APPS TO SET WEBHOOK. CHANGE <your app name> WITH YOUR HEROKU APP NAME.",
      "required":true,
      "value":"https://<your app name>.herokuapp.com"
    },
    "WEBHOOK" : {
      "description":"RUNNING MODE (BECAUSE DEPLOY ON HEROKU, THEN THE RIGHT MODE IS THE WEBHOOK.) DO NOT REMOVE THIS PART",
      "value":"true"
    },
    "OWNER_ID" : {
      "description":"USER_ID FROM OWNER BOT. THIS WILL MAKE OWNER AS A SUDO USER.",
      "required":true
    },
    "BETA":{
      "description" : "VERSION",
      "value" : "false"
    },
    "API_HASH": {
      "required": true
    },
    "API_ID": {
      "required": true
    }
  }
}